{
  "scripts": {
    "docs": "vitepress init",
    "docs:dev": "env-cmd -x vitepress dev --host=$VITE_HOST --port=$VITE_DOCS_PORT",
    "docs:build": "vitepress build",
    "docs:preview": "env-cmd -x vitepress preview --host=$VITE_HOST --port=$VITE_DOCS_PORT",
    "docs:serve": "env-cmd -x vitepress serve --port=$VITE_DOCS_PORT"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "vitepress": "^1.0.0-alpha.60"
  }
}
